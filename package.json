{
    "name": "zegocloud_client_sdk_web",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@zegocloud/zego-uikit-prebuilt": "^1.0.1",
        "clipboard": "^2.0.11",
        "clsx": "^1.2.1",
        "css-loader": "^6.7.1",
        "file-loader": "^6.2.0",
        "flv.js": "^1.6.2",
        "gh-pages": "^4.0.0",
        "http-proxy-middleware": "^2.0.6",
        "polyfill-object.fromentries": "^1.0.1",
        "protobufjs": "^7.0.0",
        "react": "^18.2.0",
        "react-app-polyfill": "^3.0.0",
        "react-dom": "^18.2.0",
        "react-intl": "^6.0.4",
        "sass-loader": "^13.0.2",
        "sass-resources-loader": "^2.2.5",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.3.1",
        "zego-express-engine-webrtc": "^2.18.0"
    },
    "homepage": "./",
    "scripts": {
        "start": "HTTPS=true HOST=0.0.0.0  REACT_APP_ENV='test' REACT_APP_PATH='live_stream' react-scripts start",
        "start:m": "REACT_APP_MOBILE='yes'   npm run start",
        "dev": "webpack serve --config webpack.config.sample.ts",
        "build:1on1_call": "REACT_APP_ENV='prd' REACT_APP_PATH='1on1_call' BUILD_PATH='1on1_call'  react-scripts build",
        "build:live_stream": "REACT_APP_ENV='prd' REACT_APP_PATH='live_stream' BUILD_PATH='live_stream'  react-scripts build",
        "build:video_conference": "REACT_APP_ENV='prd' REACT_APP_PATH='video_conference' BUILD_PATH='video_conference'  react-scripts build",
        "build": "npm run build:1on1_call & npm run build:live_stream & npm run build:video_conference",
        "deploy": "gh-pages -b main -d build",
        "eject": "react-scripts eject",
        "build:lib": "webpack",
        "publish:beta": "cd ZegoUIKitPrebuilt && npm publish --tag beta",
        "publish": "cd ZegoUIKitPrebuilt && npm publish --access public"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@formatjs/cli": "^5.0.2",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.45",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/webpack": "^5.28.0",
        "clean-webpack-plugin": "^4.0.0",
        "eslint-config-react-app": "^7.0.1",
        "father": "^2.30.21",
        "filemanager-webpack-plugin": "^7.0.0",
        "jest-dom": "^4.0.0",
        "node-sass": "^7.0.1",
        "react-scripts": "5.0.1",
        "react-testing-library": "^8.0.1",
        "resolve-url-loader": "^5.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4",
        "url-loader": "^4.1.1",
        "web-vitals": "^2.1.4",
        "webpack-cli": "^4.10.0"
    }
}
